
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/unisoc_wifi'
	option channel '149'
	option band '5g'
	option htmode 'VHT80'
	option cell_density '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'
